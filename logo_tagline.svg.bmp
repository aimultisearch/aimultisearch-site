    <line x1="86" y1="86" x2="112" y2="112" stroke="#1F56A6" stroke-width="10" stroke-linecap="round"/>

    <!-- Réseau interne -->
    <circle cx="50" cy="45" r="4" fill="#1F56A6"/>
    <circle cx="70" cy="45" r="4" fill="#1F56A6"/>
    <circle cx="60" cy="70" r="4" fill="#1F56A6"/>

    <line x1="50" y1="45" x2="70" y2="45" stroke="#1F56A6" stroke-width="3"/>
    <line x1="50" y1="45" x2="60" y2="70" stroke="#1F56A6" stroke-width="3"/>
    <line x1="70" y1="45" x2="60" y2="70" stroke="#1F56A6" stroke-width="3"/>

    <!-- Texte -->
    <text x="150" y="75" font-size="44" class="title" fill="#1F56A6">
        AI Multi Search
    </text>

